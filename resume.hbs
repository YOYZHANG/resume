<!DOCTYPE html>
<html lang="en">
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="icon" href="" type="image/svg+xml">
  <link rel="icon" href="" type="image/png">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
  <title>zhangxiaoqian's resume</title>
  <script src="https://code.iconify.design/iconify-icon/1.0.8/iconify-icon.min.js"></script>
  <link href="https://fonts.googleapis.com/css?family=Noto+Sans+SC|Nunito+Sans:400,700&display=swap" rel="stylesheet">
   <style>{{{css}}}</style>
</html>
<body itemscope itemtype="http://schema.org/Person">
   <div class="container">
      <section class="row">
        <div class="card-wrapper">
          {{#resume.basics}}
          <div class="card">
            <div class="profile-card">
                <!--<div class="profile-pic">
                  <img class="media-object img-circle center-block"  data-src="holder.js/100x100"
                    alt="{{name}}" src="{{picture}}" itemprop="image">
                </div>
                -->
                <div class="contact-details">
                  <div class="name-and-profession">
                    <h3 class="text-bolder name" itemprop="name"> {{name}}</h3>
                    <div class="text-muted job" itemprop="jobTitle">{{age}}</div>
                    <div class="text-muted job" itemprop="jobTitle">{{label}}</div>
                  </div>

                  <div class="contact-detail">
                     {{#if address}}
                        <div class="text-muted info" itemprop="address" style="font-size:0.9em">{{address}}</div>
                      {{/if}}

                      {{#if email}}
                        <div class="detail">
                          <span class="info">
                            <a href="mailto:{{email}}" class="link-disguise" itemprop="email" target="_blank">Email: {{email}}</a>
                          </span>
                        </div>
                      {{/if}}

                      {{#if phone}}
                        <div class="detail">
                          <a href="tel://{{phone}}" class="link-disguise info" itemprop="telephone" target="_blank">
                            Tel:{{phone}}
                          </a>
                        </div>
                      {{/if}}

                      {{#if blog}}
                        <div class="detail">
                          <a href="{{blog}}" class="link-disguise info" target="_blank">
                            Personal Page: {{blog}}
                          </a>
                        </div>
                      {{/if}}
                  </div>

                 

                  {{#if location}}
                    <div class="detail">
                      {{#location}}
                        <span class="info">
                          {{city}}, {{region}},
                          {{countryCode}}
                        </span>
                      {{/location}}
                    </div>
                  {{/if}}
                </div>

              <div class="social-links">
                {{#profiles}}
                  <a class="social-link icon" href="{{url}}" target="_blank">
                     <iconify-icon icon="{{toSocialIcon network}}"></iconify-icon>
                    <!--<span class="social-link-text print-only">{{username}}</span>-->
                  </a>
                {{/profiles}}
              </div>
            </div>
          </div>
          {{/resume.basics}}

        </div>
      </section>

      <section class="row page">
        <div class="card-wrapper">
          <div class="card background-card">

            <h4 id="about"></span> <span class="title">{{toLocaleTitle 'summary'}}</span> </h4>
            <div class="card-nested" itemprop="description">
              <p> {{breaklines resume.basics.summary}} </p>
            </div>

            <div id="work-projects-group">
              <div id="work-experience">
                <h4> <span class="title">{{toLocaleTitle 'work_experience'}}</span> </h4>
                <ul class="list-unstyled">
                  {{#each resume.work}}
                    <li class="card-nested relax">
                      <div class="content has-sidebar">
                        <p class="clear-margin-sm">
                          <strong>{{#if website}}<a href="{{website}}" target="_blank">{{company}}</a>{{else}}{{company}}{{/if}}</strong>
                          <span class="text-muted" style="margin-left: 0.3em">{{position}}</span>
                        </p>
                        <p class="text-muted">
                          <small>
                            <span class="space-right"> {{getBuildDate startDate}} - {{#if endDate}}{{getBuildDate endDate}}{{else}}Present{{/if}} | {{location}}</span>
                          </small>
                        </p>
                        <p>{{summary}}</p>
                        {{#if highlights}}
                          <div class="hightlight-container">
                            <ul>
                              {{#each highlights}}
                                <p class="subtitle">
                                  <strong>{{#if website}}<a href="{{website}}" target="_blank">{{topic}}</a>{{else}}{{topic}}{{/if}}</strong>
                                  <span>
                                      <small>{{skills}}</small>
                                      <small>{{startTime}} - {{endTime}}</small> - 
                                      <small>{{role}}</small>
                                  </span>
                                </p>
                                <small>{{summary}}</small>
                                <ul class='highlight'>
                                  {{#each keypoints}}
                                      <li> {{autoHighlight .}} </li>
                                      {{/each}}
                                </ul>
                                {{#if @last}}
                                    {{else}}
                                    <div style="background-color: #4A90E2;height: 1px;margin-top: 1rem; "></div>
                                {{/if}}
                              {{/each}}
                            </ul>
                          </div> 
                        {{/if}}
                      </div>

                    </li>
                  {{/each}}
                </ul>
              </div>
               {{#if resume.projects}}
              <div id="projects">
                <h4><span class="title">{{toLocaleTitle 'projects'}}</span></h4>
                <div class="list-unstyled projects">
                  <url class="project-table list-unstyled">
                  {{#each resume.projects}}
                    <li class="card-nested {{domClasses}}">
                      <div class="content has-sidebar">
                        <p class="clear-margin-sm" itemprop="award">
                          <strong>{{displayName}}</strong>&nbsp
                          {{#if githubUrl}}
                            <a href="{{githubUrl}}" class='icon' target="_blank">
                              <iconify-icon icon="{{toSocialIcon 'GitHub'}}"></iconify-icon>
                            </a>
                          {{/if}}
                          {{#if website}}
                            <a href="{{website}}" class='icon' target="_blank">
                              <iconify-icon icon="{{toSocialIcon 'link'}}"></iconify-icon>
                            </a>
                          {{/if}}
                        </p>
                        <p class="text-muted">
                          {{summary}}
                        </p>
                      </div>
                    </li>
                  {{/each}}
                  </url>
                  {{#if resume.basics.projects_url}}
                    <li class="card-nested text-muted">
                      <a href="{{resume.basics.projects_url}}" target="_blank">More Projects...</a>
                    </li>
                  {{/if}}
                  <div class='px10'></div>
                </div>
              </div>
              {{/if}}
            </div>

            <h4 id="skills"><span class="title">{{toLocaleTitle 'stacks'}}</span> </h4>
            <div class="card-nested" itemprop="description">
              <div class="skill-table">
                {{#each resume.skills}}
                    <div class='name'>{{name}}</div> 
                    <div>{{join keywords}}</div>
                {{/each}}
              </div>
            </div>
            <div class='px10'></div>

            <div id="education">
              <h4><span class="title">{{toLocaleTitle 'education'}}</span></h4>
              <ul class="list-unstyled">
                {{#each resume.education}}
                  <li class="card-nested">
                    <div class="content has-sidebar education-display">
                      <p class="clear-margin-sm">
                        <strong>{{#if area}} {{area}}, {{/if}}{{studyType}}</strong>,&nbsp;
                        {{institution}}
                      </p>
                      <div style="flex: 1 1 auto;"></div>
                      <p class="text-muted clear-margin-sm">
                        <small>
                          {{getBuildDate startDate}} - {{#if endDate}} {{getBuildDate endDate}} {{else}} Currently Pursuing {{/if}}
                        </small>
                      </p>
                      <div>{{description}}</div>
                      <i>{{gpa}}</i>
                      <div class="space-top labels">
                        {{#courses}} <span class="label label-keyword">{{.}}</span> {{/courses}}
                      </div>
                    </div>
                  </li>
                {{/each}}
              </ul>
            </div>


            {{#if resume.publications}}
            <h4 id="publications"><span class="title">Talks</span> </h4>
            <ul class="list-unstyled">
              {{#each resume.publications}}
                <li class="card-nested" style="margin-bottom: 0.5em">
                  <div class="content has-sidebar">
                    <p class="clear-margin-sm">
                      <strong>{{#if website}}<a href="{{website}}" target="_blank">{{name}}</a>{{else}}{{name}}{{/if}}</strong>,&nbsp;
                      {{publisher}}
                    </p>
                    <p class="text-muted clear-margin-sm">
                      <small> {{releaseDate}} {{dateNote}}</small>
                      <small>{{#if slides}},&nbsp;<a href="{{slides}}" target="_blank">Slides</a>{{/if}}</small>
                    </p>
                    <p class="clear-margin">{{summary}}</p>
                  </div>
                </li>
              {{/each}}
            </ul>
            {{/if}}

            {{#if resume.awards}}
            <h4 id="awards"><span class="title">Awards</span></h4>
            <ul class="list-unstyled">
              {{#each resume.awards}}
                <li class="card-nested">
                  <div class="content has-sidebar">
                    <p class="clear-margin-sm" itemprop="award">
                      <strong>{{title}}</strong>,&nbsp;
                      {{awarder}}
                    </p>
                    <p class="text-muted clear-margin-sm">
                      <small> {{date}} </small>
                    </p>
                    {{#if summary}}<p> {{summary}} </p>{{/if}}
                  </div>
                </li>
              {{/each}}
            </ul>
            {{/if}}

            {{#if resume.volunteer}}
            <h4 id="volunteer-work"><span class="title">Volunteer Work</span> </h4>
            <ul class="list-unstyled">
              {{#each resume.volunteer}}
                <li class="card-nested">
                  <div class="content has-sidebar">
                    <p class="clear-margin-sm">
                      <strong>{{position}}</strong>,&nbsp;
                      {{organization}}
                    </p>
                    <p class="text-muted">
                      <small>
                        {{startDate}} - {{#if endDate}} {{endDate}} {{else}} Present {{/if}}
                      </small>
                    </p>
                    <p>{{summary}}</p>
                    <ul>
                      {{#each highlights}}
                        <p class="hight-topic">
                          <strong>{{topic}}</strong>
                          <small>{{lang}}</small>
                        </p>
                        {{#each keypoints}}
                          <li> {{.}} </li>
                        {{/each}}
                      {{/each}}
                    </ul>
                  </div>
                </li>
              {{/each}}
            </ul>
            {{/if}}

            {{#if resume.interests}}
            <h4 id="interests"><span class="title">Interests</span> </h4>
            <ul class="list-unstyled">
              {{#each resume.interests}}
                <li class="card-nested">
                  <p>
                    <strong>{{name}}</strong>
                  </p>

                  <div class="space-top labels">
                    {{#keywords}}
                      <span class="label label-keyword">{{.}}</span>
                    {{/keywords}}
                  </div>
                </li>
              {{/each}}
            </ul>
            {{/if}}

            {{#if resume.references}}
            <h4 id="references"><span class="title">References</span> </h4>
            <ul class="list-unstyled">
              {{#each resume.references}}
                <li class="card-nested">
                  <p>
                    <strong>{{name}}</strong>
                  </p>
                  <blockquote class="quote">
                    <p class="clear-margin">{{reference}}</p>
                  </blockquote>
                </li>
              {{/each}}
            </ul>
            {{/if}}

            {{#if resume.languages}}
            <h4 id="languages"><span class="title">{{toLocaleTitle 'languages'}}</span> </h4>
            <p class="card-nested">
              {{#each resume.languages}}
                <span class="enumeration"><strong>{{language}}</strong> ({{fluency}})</span>
              {{/each}}
            </p>
            {{/if}}

            <div style="text-align:center">
              <div class="detail no-print" style="margin-top:2rem;">
                <a href="{{getPdfLinks}}" class="info" itemprop="pdf" target="_blank">
                  PDF Version
                </a>
              </div>
               <div class="detail print-only" style="font-size:1rem;" >
                <a href="{{getOnlineLinks}}" class="info" target="_blank">
                  Online Version
                </a>
              </div>
            </div>
          </div>
        </div>
      </section>
    </div>

</body>
